.VARi i 4
.ARRAYi arr 3 5
.ARRAYi arr2 2 3 4

.FUNC test x
	LDi $i6 x
	ADDIi $i7 $i6 10
	CALL_RET test2 $i8 $i7
	RETi $i8

.FUNC test2 y
	LDi $i10 y
	MULIi $i11 $i10 3
	RETi $i11
	
.FUNC gross x y z a b c
	LDi $i13 x
	LDf $f0 y
	LDi $i14 z
	LDf $f1 a
	LDi $i15 b
	LDf $f2 c
	
	ADDi $i16 $i13 $i14
	ADDi $i16 $i16 $i15
	ADDf $f3 $f0 $f1
	ADDf $f3 $f3 $f2
	
	ITOF $f4 $i16
	ADDf $f4 $f4 $f3
	RETf $f4
	
.FUNC weird
	LDIi $i5 3
	LDIi $i6 2
	LDIi $i7 1
	
	LDIi $i8 2
	
	LDi $i0 arr2
	
LOOP:
	BRZ $i8 END
	LDRi $i1 $i0
	
	LDRi $i2 $i1
	STRi $i5 $i2 0
	STRi $i5 $i2 1
	STRi $i5 $i2 2
	STRi $i5 $i2 3
	
	LDRi $i2 $i1 1
	STRi $i6 $i2 0
	STRi $i6 $i2 1
	STRi $i6 $i2 2
	STRi $i6 $i2 3
    
    LDRi $i2 $i1 2
	STRi $i7 $i2 0
	STRi $i7 $i2 1
	STRi $i7 $i2 2
	STRi $i7 $i2 3
	
	ADDIi $i0 $i0 1
	SUBIi $i8 $i8 1
	BR LOOP
	
END:
	RET

.FUNC main
	LDi $i0 i
	LDIi $i1 38
	ADDi $i1 $i0 $i1
	
	LDi $i2 arr
	ADDIi $i3 $i2 2
	LDRi $i4 $i3
	STRi $i1 $i4
	
	CALL_RET readi $i12
	BRZ $i12 skip
	
	ADDIi $i1 $i1 1
	RET
	
skip:
	CALL printi $i1
	CALL_RET test $i9 $i1
	CALL printi $i9

	LDIi $i0 1
	LDIi $i1 2
	LDIi $i2 3
	LDIf $f0 4.5
	LDIf $f1 5.5
	LDIf $f2 10.1
	CALL_RET gross $f5 $i0 $f0 $i1 $f1 $i2 $f2
	CALL printf $f5
	
	CALL weird

	RET
