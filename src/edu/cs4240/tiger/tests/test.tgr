let
	type hello := int;
	type world := float;
	
	var a, b, c : hello := 9;
	var d, e, f : world := 42.0;
	
	var g, i, j : bool := false;
	
	var h : array[10] of hello;
	
	var wat : array[5] of array[5] of hello;
	
	func myFunc(arg1 : int, arg2 : world) : world begin
		return h[4];
	end;
	
	func test(a : int) : int begin
		h[4] := h[4];
		a := 1000;
		myFunc(3, 4);
		
		if(g & i | j | a < 100) then
			for a := 0 to 9 do
				printi(42);
			enddo;
		endif;
		
		if 3 = 4 then
			printi(3);
		endif;
		
		while d = ((4.3) * (55 + 32) / (122.2 - 44)) do
			h[3] := 4;
		enddo;
		
		for b := 44 to 69 do
			myFunc(b, 3.0);
			
			for c := 1 to 100 do
				myFunc(c, 1337);
				break;
			enddo;
			
			break;
		enddo;
		
		printi(2);
		return 2;
	end;
	
	func printi() begin
		h[4] := 10000;
	end;
in
	h[4] := h[4];
	myFunc(3, 4);
	
	f := myFunc(a, f);
	
	h := wat[3];
	a := h[4];
	
	if 3 = 4 then
		printi(3);
	endif;
	
	while d = 4.3 do
		h[3] := 4;
	enddo;
	
	for b := 44 to 69 do
		myFunc(b, 3.0);
		
		for c := 1 to 100 do
			myFunc(c, 1337);
			break;
		enddo;
		
		break;
	enddo;
end
